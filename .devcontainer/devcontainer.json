{
	"image": "mcr.microsoft.com/dotnet/runtime-deps:8.0.6-bookworm-slim",
  "customizations": {
    "vscode": {
	    "extensions": [
	    	"ms-dotnettools.csharp"
	    ]
	  }
	},
  "features": {
    "ghcr.io/dragos-tudor/devcontainer-features/debian-packages:1.3": {},
    "ghcr.io/dragos-tudor/devcontainer-features/debian-user-group:1.5": {},
    "ghcr.io/dragos-tudor/devcontainer-features/debian-dotnet-product:1.4": { "version": "8.0.302" }
  },
	"runArgs": [
		"--name=backend-storing",
		"--hostname=storing",
		"--net=storing-network"
	],
  "containerEnv": {
    "DOCKER_HOST_PORT": "${localEnv:DOCKER_HOST_PORT:2375}",
    "DOCKER_HOST_GATEWAY": "${localEnv:DOCKER_HOST_GATEWAY:172.17.0.1}"
  },
	"remoteUser": "vscode"
}
