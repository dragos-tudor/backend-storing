{
	"image": "ubuntu:22.04",
  "customizations": {
    "vscode": {
	    "extensions": [
	    	"ms-dotnettools.csharp"
	    ]
	  }
	},
  "features": {
    "ghcr.io/dragos-tudor/devcontainer-features/debian-packages:1.2": {},
    "ghcr.io/dragos-tudor/devcontainer-features/debian-user-group:1.4": {},
    "ghcr.io/dragos-tudor/devcontainer-features/debian-dotnet-product:1.2": {}
  },
	"mounts": [
		"source=nuget,target=/home/vscode/.nuget"
	],
	"runArgs": [
		"--name=backend-storing",
		"--hostname=storing",
		"--net=host"
	],
	"remoteUser": "vscode"
}
